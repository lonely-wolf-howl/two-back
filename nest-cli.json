{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api-gateway",
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user",
      "entryFile": "main",
      "sourceRoot": "apps/user/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user/tsconfig.app.json"
      }
    },
    "challenge-service": {
      "type": "application",
      "root": "apps/challenge",
      "entryFile": "main",
      "sourceRoot": "apps/challenge/src",
      "compilerOptions": {
        "tsConfigPath": "apps/challenge/tsconfig.app.json"
      }
    },
    "record-service": {
      "type": "application",
      "root": "apps/record",
      "entryFile": "main",
      "sourceRoot": "apps/record/src",
      "compilerOptions": {
        "tsConfigPath": "apps/record/tsconfig.app.json"
      }
    }
  }
}
